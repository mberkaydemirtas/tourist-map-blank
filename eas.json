{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },

  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": { "buildType": "apk" },
        "env": {
  "EXPO_PUBLIC_API_BASE": "http://localhost:5000",
  "EXPO_PUBLIC_API_TIMEOUT_MS": "20000"
}
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": true
    },
    "production": {
      "android": { "buildType": "app-bundle" },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
